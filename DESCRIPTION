  checkmate,
  digest,
  dplyr,
  lubridate,
  methods,
  ParallelLogger (>= 3.0.0),
  readr (>= 2.1.0),
  rlang,
  RJSONIO,
  jsonlite,
  ResultModelManager,
  SqlRender (>= 1.11.1),
  stringi (>= 1.7.6),
  tibble
